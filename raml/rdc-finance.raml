      {
          "type": "array",
          "name": "CashOutSummaryReportData",
          "$schema": "http://json-schema.org/draft-03/schema",
          "description": "",
          "properties": { 
            "CashOutID": { "type": "integer", "database": "INT", "example": "13775", "description": "Internal identifier in RQ" },  
            "CashOutIDByStore": { "type": "string", "database": "VARCHAR(14)", "example": "84WEACA649", "description": "Cashout # - number that the system generated for the cashout at the time it was created. To view the cashout simply click its underlined link" },  
            "StoreName": { "type": "string", "database": "NVARCHAR(MAX)", "example": "84 West", "description": "Name of the store" },  
            "Reconciled": { "type": "string", "database": "VARCHAR(3)", "example": "No", "description": "A flag to indicate if the cashout was reconciled" },  
            "StartDate": { "type": "datetime", "database": "DATETIME", "example": "2012-08-20T18:18:52.023", "description": "Started On - start time of this cashout's interval. If this is the first cashout for this location then the Started On Date will be N/A" },  
            "StopDate": { "type": "datetime", "database": "DATETIME", "example": "2015-02-24T13:40:33.21", "description": "Stopped On - stop time of this cashout's interval" },  
            "EmployeeName1": { "type": "string", "database": "VARCHAR(51)", "example": "iQmetrix Employee", "description": "Employee 1 - name of the employee who was logged in when the cashout was done" },  
            "EmployeeUsername1": { "type": "string", "database": "VARCHAR(51)", "example": "iQmetrix.Employee", "description": "Username1 - username of the employee who was logged in when the cashout was done" },  
            "EmployeeName2": { "type": "string", "database": "VARCHAR(51)", "example": "iQmetrix Employee", "description": "Employee 2 - name of the employee who verified the cashout was correct, this may be the same person as Employee 1" },  
            "EmployeeUsername2": { "type": "string", "database": "VARCHAR(51)", "example": "iQmetrix.Employee", "description": "Username2 - the username of the employee who was logged in when the cashout was done" },  
            "PaymentName": { "type": "string", "database": "NVARCHAR(MAX)", "example": "Cash", "description": "Payment - payment type for the row's totals" },  
            "CreditCardPaymentType": { "type": "string", "database": "VARCHAR(20)", "example": "", "description": "Credit card payment type" },  
            "PaymentReconciled": { "type": "string", "database": "VARCHAR(3)", "example": "No", "description": "Payment Reconciled - indicates whether the payment type has been reconciled or not" },  
            "Cashout": { "type": "string", "database": "VARCHAR(5000)", "example": "", "description": "Cashout Comments - comments from the cashout" },  
            "Reconciliation": { "type": "string", "database": "VARCHAR(1000)", "example": "", "description": "Reconciliation Comments - from the reconciliation if it has been done" },  
            "DepositNumber": { "type": "string", "database": "VARCHAR(50)", "example": "100.00", "description": "Deposit # - number for this particular deposit, e.g. bank deposit number" },  
            "ActualTotal": { "type": "decimal", "database": "MONEY", "example": "1884.1", "description": "Your Total - total filled in by Employee 1 from the till totals for all transactions during the cashoutinterval for the row's payment type" },  
            "SystemTotal": { "type": "decimal", "database": "MONEY", "example": "0", "description": "System Total - overall total the software has recorded for all transactions during the cashout interval for the row's payment type. This includes all payments accepted for sales, bill pay and for customer account payments" },  
            "Skim": { "type": "decimal", "database": "MONEY", "example": "0", "description": "Internal value in RQ" },  
            "Float": { "type": "decimal", "database": "MONEY", "example": "0", "description": "Internal value in RQ" },  
            "DifferenceTotal": { "type": "decimal", "database": "MONEY", "example": "1884.1", "description": "Difference - Your Total - System Total" },  
            "TransactionQuantity": { "type": "integer", "database": "INT", "example": "0", "description": "Internal value in RQ" },  
            "CashAndCheque": { "type": "decimal", "database": "MONEY", "example": "1884.1", "description": "Cash/check (cheque) - total for the cash and check payment types" },  
            "FloatRetained": { "type": "decimal", "database": "MONEY", "example": "0", "description": "Float Retained - dollar amount retained as a float" },  
            "DepositAmount": { "type": "decimal", "database": "MONEY", "example": "1884.1", "description": "Deposit - actual deposit value for that cashout" },  
            "CashInAmount": { "type": "decimal", "database": "MONEY", "example": "0", "description": "Cashin - cashed in float amount" },  
            "BankDetails": { "type": "string", "database": "NVARCHAR(MAX)", "example": "", "description": "Bank Details - states the bank associated to the cashout" }  
          }  
        }

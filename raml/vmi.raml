        "properties": {
          "VendorID": { "type": "GUID", "description": "Vendor identifier", "example": "9DC6AA95-856B-42C9-8AAF-392A2A02AC77" }, 
          "Username": { "type": "String", "description": "Username", "example": "sampleusername" }, 

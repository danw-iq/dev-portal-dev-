                    application/JsonDataTable:
                        
                            
                            example: |
                                [{"CategoryNumber":"10101012","Category":"Financing","CategoryPath":">> Activations >> Financing"}]
                            schema: |
                                {"name":"Category","type":"array","$schema":"http://json-schema.org/draft-03/schema","properties":{"CategoryNumber":{"type":"string","description":"Identifier for the Category","database":"NVARCHAR (100)","example":"10101012"},"Category":{"type":"string","description":"Category Name","database":"NVARCHAR (MAX)","example":"Financing"},"CategoryPath":{"type":"string","description":"Category","database":"NVARCHAR (MAX)","example":">> Activations >> Financing"}}}
                            
                        
                    
                    text/xml:
                        
                            
                            example: |
                                [{"CategoryNumber":"10101012","Category":"Financing","CategoryPath":">> Activations >> Financing"}]
                            schema: |

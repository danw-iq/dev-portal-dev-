#%RAML 0.8
title: Billing Reports
baseUri: https://rqdataconnectdemo.iqmetrix.net/private/
schemas: 

    
    - SaleInvoice: |
        {
            "type": "array",
            "name": "SaleInvoice",
            "$schema": "http://json-schema.org/draft-03/schema",
            "description": "",
            "properties": { 
              "CustomerId": { "type": "integer", "database": "INT", "example": "13699", "description": "Identifier for a customer" },  
              "InvoiceId": { "type": "integer", "database": "INT", "example": "1", "description": "Identifier for this invoice" },  
              "PaymentTypeId": { "type": "integer", "database": "INT", "example": "3", "description": "Identifier for a Payment Type" },  
              "AccountsReceivableId": { "type": "integer", "database": "INT", "example": "68", "description": "" },  
              "AccountsReceivableName": { "type": "string", "database": "VARCHAR(14)", "example": "TTL10AR1", "description": "" },  
              "InvoiceName": { "type": "string", "database": "VARCHAR(14)", "example": "TARGEIN1", "description": "Invoice name" },  
              "CustomerName": { "type": "string", "database": "VARCHAR(50)", "example": "KENTEL corp", "description": "Customer name" },  
              "DatabaseName": { "type": "string", "database": "VARCHAR(25)", "example": "KENTE", "description": "Database name" },
              "TotalAmount": { "type": "decimal", "database": "MONEY", "example": "38.14", "description": "Total amount" },  
              "TotalPaid": { "type": "decimal", "database": "MONEY", "example": "50", "description": "Total paid" },  
              "CreatedDate": { "type": "datetime", "database": "DATETIME", "example": "2010-07-03T11:07:19.59", "description": "Created date" },  
              "OriginalId": { "type": "integer", "database": "INT", "example": "1", "description": "Identifier of the original invoice" }  
            }  
          }
        

    

    
    - EnabledRecurringInvoiceData: |
        {
            
            "type": "array",
            "name": "EnabledRecurringInvoiceData",
            "$schema": "http://json-schema.org/draft-03/schema",
            "properties": { 
              "CustomerId": { "type": "integer", "database": "INT", "example": "124059", "description": "Identifier for a customer" },  
              "AccountsReceivableId": { "type": "integer", "database": "INT", "example": "68", "description": "" },  
              "AccountsReceivableName": { "type": "string", "database": "VARCHAR(14)", "example": "TTL10AR1", "description": "" },  
              "StartDate": { "type": "datetime", "database": "DATETIME", "example": "2014-12-16T00:00:00", "description": "Start date" },  
              "StopDate": { "type": "datetime", "database": "DATETIME", "example": "", "description": "End date" },  
              "RecurringInvoiceId": { "type": "integer", "database": "INT", "example": "8", "description": "Identifier for the recurring invoice" },  
              "RecurringInvoiceName": { "type": "string", "database": "VARCHAR(14)", "example": "ML101RI1", "description": "Recurring invoice name" },  
              "ProductId": { "type": "integer", "database": "INT", "example": "5002", "description": "Product Id" },  
              "Quantity": { "type": "integer", "database": "INT", "example": "1", "description": "Quantity" },  
              "Price": { "type": "decimal", "database": "MONEY", "example": "100", "description": "Price" },  
              "Sku": { "type": "string", "database": "VARCHAR(12)", "example": "ASBANS000070", "description": "SKU" },  
              "Description": { "type": "string", "database": "NVARCHAR(MAX)", "example": "NSP", "description": "Description" }  
            }  
          }
        

    

    
    - InvoicesWithUnpaidBalanceData: |
        {
            "exclude": "true",
            "type": "array",
            "name": "InvoicesWithUnpaidBalanceData",
            "$schema": "http://json-schema.org/draft-03/schema",
            "properties": { 
              "InvoiceId": { "type": "integer", "database": "INT", "example": "130313", "description": "Identifier for an invoice" }  
            }  
          }
        

    

    
    - SaleInvoicesByProductFilterData: |
        {
            
            "type": "array",
            "name": "SaleInvoicesByProductFilterData",
            "$schema": "http://json-schema.org/draft-03/schema",
            "properties": { 
              "CustomerId": { "type": "integer", "database": "INT", "example": "2922", "description": "Identifier for a customer" },  
              "SaleInvoiceId": { "type": "integer", "database": "INT", "example": "2", "description": "Identifier for the sale invoice" },  
              "Quantity": { "type": "integer", "database": "INT", "example": "1", "description": "Quantity" },  
              "Price": { "type": "decimal", "database": "MONEY", "example": "49.99", "description": "Price" },  
              "AccountsReceivableId": { "type": "integer", "database": "INT", "example": "5", "description": "" },  
              "AccountsReceivableName": { "type": "string", "database": "VARCHAR(14)", "example": "", "description": "" },  
              "Year": { "type": "integer", "database": "INT", "example": "2011", "description": "Year (full)" },  
              "Month": { "type": "integer", "database": "INT", "example": "1", "description": "Month as an integer" },  
              "OriginalId": { "type": "integer", "database": "INT", "example": "0", "description": "Original invoice id" },  
              "RefundCodeId": { "type": "GUID", "database": "UNIQUEIDENTIFIER", "example": "159c193f-1904-deaa-2fed-a318b17f2b70", "description": "Identifier for a refund code" },  
              "SaleInvoiceName": { "type": "string", "database": "VARCHAR(14)", "example": "84WEAIN1", "description": "Sale invoice name" },  
              "CustomerName": { "type": "string", "database": "VARCHAR(50)", "example": "KENTEL corp", "description": "Customer name" },  
              "DatabaseName": { "type": "string", "database": "VARCHAR(25)", "example": "KENTE", "description": "Database name" }  
            }  
          }
        

    

    
    - SummaryOfSaaSProductData: |
        {
            
            "type": "array",
            "name": "SummaryOfSaaSProductData",
            "$schema": "http://json-schema.org/draft-03/schema",
            "properties": { 
              "CustomerID": { "type": "integer", "database": "INT", "example": "85161", "description": "Identifier for a customer" },  
              "LocationCount": { "type": "integer", "database": "INT", "example": "0", "description": "Number of locations" },  
              "WorkstationCount": { "type": "integer", "database": "INT", "example": "0", "description": "Number of workstations" },  
              "RqMobileCount": { "type": "integer", "database": "INT", "example": "0", "description": "Number of RQ Mobile devices" }  
            }  
          }
        

    

    
    - InvoicesWithMatchingEpinIDData: |
        {
            
            "type": "array",
            "name": "InvoicesWithMatchingEpinIDData",
            "$schema": "http://json-schema.org/draft-03/schema",
            "properties": { 
              "InvoiceID": { "type": "string", "database": "VARCHAR(14)", "example": "84WEAIN16524", "description": "Identifier for an invoice" },  
              "ProductSKU": { "type": "string", "database": "VARCHAR(12)", "example": "INEPNS000001", "description": "Product SKU" },  
              "SaleInvoiceID": { "type": "integer", "database": "INT", "example": "130511", "description": "Identifier for an invoice" },  
              "Priority": { "type": "integer", "database": "INT", "example": "1", "description": "Priority" },  
              "PinTypeID": { "type": "integer", "database": "INT", "example": "21", "description": "Identifier for a pin type" },  
              "PinCode": { "type": "string", "database": "VARCHAR(30)", "example": "9875564858978", "description": "EPIN pin code" },  
              "SerialNumber": { "type": "string", "database": "VARCHAR(100)", "example": "987465328", "description": "Serial number" },  
              "Amount": { "type": "decimal", "database": "MONEY", "example": "50", "description": "Amount" },  
              "CostToCustomer": { "type": "decimal", "database": "MONEY", "example": "50", "description": "Customer cost" }  
            }  
          }
        

    

    
    - TrafficCount: |
        {
            
            "exclude": "true",
            "type": "array",
            "name": "TrafficCount",
            "$schema": "http://json-schema.org/draft-03/schema",
            "properties": { 
              "storeid": { "type": "integer", "database": "INT", "example": "1", "description": "Identifier for a store in RQ" }  
            }  
          }
        

    

    
    - InvoicesPaidToAccountData: |
        {         
            "type": "array",
            "name": "InvoicesPaidToAccountData",
            "$schema": "http://json-schema.org/draft-03/schema",
            "properties": { 
              "CustomerId": { "type": "integer", "database": "INT", "example": "54514", "description": "Identifier for a customer" },  
              "SaleInvoiceId": { "type": "integer", "database": "INT", "example": "1891", "description": "" },  
              "ProductId": { "type": "integer", "database": "INT", "example": "1355", "description": "Identifier for a product" },  
              "Quantity": { "type": "integer", "database": "INT", "example": "1", "description": "Quantity" },  
              "Price": { "type": "decimal", "database": "MONEY", "example": "79.99", "description": "Price" },  
              "Sku": { "type": "string", "database": "VARCHAR(12)", "example": "ASMIRI000016", "description": "SKU" },  
              "Description": { "type": "string", "database": "NVARCHAR(MAX)", "example": "Blackberry Curve 3 Grey", "description": "Description" }  
            }  
          }
        

    

    
    - GlobalProduct: |
        {
            "type": "array",
            "name": "GlobalProduct",
            "$schema": "http://json-schema.org/draft-03/schema",
            "properties": { 
              "GlobalProductID": { "type": "integer", "database": "INT", "example": "1", "description": "Identifier for a <a href='http://developers.iqmetrix.com/api/Billing-Reports/#globalproduct'>GlobalProduct</a>" },  
              "ProductIdentifier": { "type": "string", "database": "VARCHAR(12)", "example": "ASCLMO000001", "description": "Product identifier" },  
              "ProductName": { "type": "string", "database": "NVARCHAR(MAX)", "example": "CLA Mot V262/265 Platinum_TestCLA", "description": "Product name" }  
            }  
          }
        

    



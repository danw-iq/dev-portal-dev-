                                [{"EmployeeID":250,"Abbreviation":"84WEA","StoreName":"84 West","FirstName":"iQmetrix","LastName":"Employee","PunchInTime":"2015-07-02T12:15:02.45","PunchOutTime":"2015-07-10T13:49:47.457","PunchInComments":""}]

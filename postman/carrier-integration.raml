/Companies({CompanyId})/Locations({LocationId})/Carriers({CarrierId})/Activations({ActivationId}):
    
    uriParameters:
        
        CompanyId:
            displayName: CompanyId
            type: integer
            example: 123
            required: true
            description: Identifier for the <a href='http://developers.iqmetrix.com/api/company-tree/#company'>Company</a>
        
        LocationId:
            displayName: LocationId
            type: integer
            example: 5678
            required: true
            description: Identifier for the <a href='http://developers.iqmetrix.com/api/company-tree/#location'>Location</a>
        
        CarrierId:
            displayName: CarrierId
            type: integer
            example: 41
            required: true
            description: Identifier for the <a href='http://developers.iqmetrix.com/api/entity-store/#carrier'>Carrier</a>
        
        ActivationId:
            displayName: ActivationId
            type: string
            example: 6=1115550123
            required: true
            description: Activation lookup string, see <a href="#getting-an-activation">Getting an Activation</a>
          
      
